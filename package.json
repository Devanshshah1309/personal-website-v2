{
  "name": "devansh",
  "type": "module",
  "version": "1.1.0",
  "description": "A minimal, SEO-friendly portfolio and blog theme for Astro, supports Vue and UnoCSS.",
  "license": "MIT",
  "keywords": [
    "devansh",
    "portfolio website",
    "blog",
    "notes",
    "projects"
  ],
  "engines": {
    "node": ">=v18.17.1 || >=v20.3.0 || >=21"
  },
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "astro dev --host",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp"
  },
  "dependencies": {
    "@astrojs/markdown-remark": "^5.3.0",
    "@astrojs/mdx": "^4.3.0",
    "@astrojs/partytown": "^2.1.2",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/vercel": "^8.1.5",
    "@astrojs/vue": "^5.1.0",
    "@supabase/supabase-js": "^2.49.9",
    "@unocss/reset": "^0.61.0",
    "astro": "^5.8.2",
    "cohere-ai": "^7.17.1",
    "katex": "^0.16.11",
    "nprogress": "^0.2.0",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "unocss": "^0.61.0",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.17.0",
    "@iconify/json": "^2.2.204",
    "@types/lodash-es": "^4.17.12",
    "@types/nprogress": "^0.2.3",
    "@vueuse/core": "^10.11.0",
    "bumpp": "^9.4.1",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-format": "^0.1.2",
    "lint-staged": "^15.2.7",
    "lodash-es": "^4.17.21",
    "prettier-plugin-astro": "^0.13.0",
    "simple-git-hooks": "^2.11.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "npm run lint:fix"
  }
}
